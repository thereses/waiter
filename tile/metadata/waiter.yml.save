---
name: Waiter
product_version: "1.0"
metadata_version: "1.7"
label: 'Ops Manager: Your Perfect Waiter'
description: "Ops MAnager installation notifier"
stemcell_criteria:
  os: ubuntu-trusty
  version: "3215"
  enable_patch_security_updates: true
releases:
  - name: waiter
    file: waiter-1.tgz
    version: "1"

form_types:
  - name: main
    label: Options
    description: Enter your details
    property_inputs:
      - reference: .waiter.om_credentials
        label: 'Ops Manager credentials'
        description: 'Credentials for OpsManager'
      - reference: .waiter.email
        label: 'Email to send notification'
        description: 'Email to send notification'
job_types:
  - name: waiter
    manifest: |
      waiter:
        email: (( .waiter.email.value ))
        username: (( .waiter.om_credentials.identity ))
        password: (( .waiter.om_credentials.password ))
    resource_label: Waiter
    templates:
      - name: waiter
        release: waiter
    release: waiter
    static_ip: 1
    dynamic_ip: 0
    max_in_flight: 1
    single_az_only: true
    instance_definition:
      name: instances
      type: integer
      configurable: false
      default: 1
      constraints:
        max: 1
    resource_definitions:
      - name: ram
        type: integer
        configurable: true
        default: 1024
      - name: ephemeral_disk
        type: integer
        configurable: true
        default: 2048
      - name: persistent_disk
        type: integer
        configurable: true
        default: 1024
        constraints:
          min: 1024
      - name: cpu
        type: integer
        configurable: true
        default: 1
    property_blueprints:
    - name: om_credentials
      type: simple_credentials
      configurable: true
      optional: true
    - name: email
      type: email
      configurable: true
      default: foo@example.com


